{
    "name": "cleaniquecoders/blade-plus-plus",
    "description": "Extended Laravel Blade Directives",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\BladePlusPlus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\BladePlusPlus\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/filesystem": "^5.5|^5.6|^5.7|^5.8",
        "illuminate/support": "^5.5|^5.6|^5.7|^5.8",
        "symfony/console": "~4.0",
        "symfony/filesystem": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0|^8.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\BladePlusPlus\\BladePlusPlusServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
